{
  "manifest_version": 2,

  "name": "RBM Github Dashboard",
  "description" : "RBM Github Username Dashboard",
  "version" : "1.0",
  "permissions": [
    "alarms",
    "storage",
    "notifications",
    "https://github.com/login/oauth/access_token",
    "https://api.github.com/*"
    ],
  "chrome_url_overrides" : {
        "newtab": "index.html"
    },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}